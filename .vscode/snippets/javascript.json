{
  "console.log": {
    "prefix": "logg",
    "body": [
      "console.log($1);"
    ]
  },
  "react component": {
    "prefix": "rc",
    "body": [
      "import React, { Component } from 'react';",
      "import { connect } from 'react-redux';",
      "import { withRouter } from 'react-router-dom';",
      "",
      "class $1 extends Component {",
      "  render = () => {",
      "    return (",
      "      <h1>$1</h1>",
      "    );",
      "  }",
      "}",
      "",
      "const reduxConsole = connect(",
      "  (state) => ({ ...state.console })",
      ")($1);",
      "export default withRouter(reduxConsole);"
    ]
  }
}
